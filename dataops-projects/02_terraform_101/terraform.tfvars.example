# Example Terraform Variables File
# Copy this file to terraform.tfvars and customize the values

environment  = "dev"
project_name = "terraform-101"

tags = {
  ManagedBy = "Terraform"
  Project   = "DataOps"
  Team      = "Data-Engineering"
}

